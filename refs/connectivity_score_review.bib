	keywords = {Genome, Bacterial, Evolution, Molecular, Escherichia coli, Genes, Bacterial, Gram-Negative Bacteria, Conserved Sequence, Genes, Essential, Helicobacter pylori, Neisseria meningitidis},
	pages = {962--968}
}


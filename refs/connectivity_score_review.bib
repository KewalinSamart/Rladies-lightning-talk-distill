	editor = {Lai, Xin and Gupta, Shailendra K. and Vera, Julio},
	year = {2019},
	doi = {10.1007/978-1-4939-8982-9_11},
	keywords = {Bipartite networks, Disease associations, Minimum dominating sets, ncRNA-protein interactions, Network controllability, Noncoding RNA},
	pages = {289--300},
	file = {Springer Full Text PDF:/Users/jananiravi/Zotero/storage/XD8NIDE6/Nacher and Akutsu - 2019 - Controllability Methods for Identifying Associatio.pdf:application/pdf}
}


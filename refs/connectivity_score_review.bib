	keywords = {Animals, Molecular Sequence Annotation, Sequence Analysis, DNA, Genome, Deer, China},
	pages = {180305}
}

@article{duffy_serum_2018,
	title = {A {Serum} {Circulating} {miRNA} {Signature} for {Short}-{Term} {Risk} of {Progression} to {Active} {Tuberculosis} {Among} {Household} {Contacts}},
	volume = {9},
	issn = {1664-3224},
	doi = {10.3389/fimmu.2018.00661},
	abstract = {Biomarkers that predict who among recently Mycobacterium tuberculosis (MTB)-exposed individuals will progress to active tuberculosis are urgently needed. Intracellular microRNAs (miRNAs) regulate the host response to MTB and circulating miRNAs (c-miRNAs) have been developed as biomarkers for other diseases. We performed machine-learning analysis of c-miRNA measurements in the serum of adult household contacts (HHCs) of TB index cases from South Africa and Uganda and developed a c-miRNA-based signature of risk for progression to active TB. This c-miRNA-based signature significantly discriminated HHCs within 6 months of progression to active disease from HHCs that remained healthy in an independent test set [ROC area under the ROC curve (AUC) 0.74, progressors {\textless} 6 Mo to active TB and ROC AUC 0.66, up to 24 Mo to active TB], and complements the predictions of a previous cellular mRNA-based signature of TB risk.},
	language = {eng},
	journal = {Frontiers in Immunology},
	author = {Duffy, Fergal J. and Thompson, Ethan and Downing, Katrina and Suliman, Sara and Mayanja-Kizza, Harriet and Boom, W. Henry and Thiel, Bonnie and Weiner Iii, January and Kaufmann, Stefan H. E. and Dover, Drew and Tabb, David L. and Dockrell, Hazel M. and Ottenhoff, Tom H. M. and Tromp, Gerard and Scriba, Thomas J. and Zak, Daniel E. and Walzl, Gerhard and {GC6-74 Consortium}},
	year = {2018},

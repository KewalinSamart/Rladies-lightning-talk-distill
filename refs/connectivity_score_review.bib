@article{coppola_new_2016,
	title = {New {Genome}-{Wide} {Algorithm} {Identifies} {Novel} {In}-{Vivo} {Expressed} {Mycobacterium} {Tuberculosis} {Antigens} {Inducing} {Human} {T}-{Cell} {Responses} with {Classical} and {Unconventional} {Cytokine} {Profiles}},
	volume = {6},
	issn = {2045-2322},
	doi = {10.1038/srep37793},
	abstract = {New strategies are needed to develop better tools to control TB, including identification of novel antigens for vaccination. Such Mtb antigens must be expressed during Mtb infection in the major target organ, the lung, and must be capable of eliciting human immune responses. Using genome-wide transcriptomics of Mtb infected lungs we developed data sets and methods to identify IVE-TB (in-vivo expressed Mtb) antigens expressed in the lung. Quantitative expression analysis of 2,068 Mtb genes from the predicted first operons identified the most upregulated IVE-TB genes during in-vivo pulmonary infection. By further analysing high-level conservation among whole-genome sequenced Mtb-complex strains (n = 219) and algorithms predicting HLA-class-Ia and II presented epitopes, we selected the most promising IVE-TB candidate antigens. Several of these were recognized by T-cells from in-vitro Mtb-PPD and ESAT6/CFP10-positive donors by proliferation and multi-cytokine production. This was validated in an independent cohort of latently Mtb-infected individuals. Significant T-cell responses were observed in the absence of IFN-γ-production. Collectively, the results underscore the power of our novel antigen discovery approach in identifying Mtb antigens, including those that induce unconventional T-cell responses, which may provide important novel tools for TB vaccination and biomarker profiling. Our generic approach is applicable to other infectious diseases.},
	language = {eng},
	journal = {Scientific Reports},
	author = {Coppola, Mariateresa and van Meijgaarden, Krista E. and Franken, Kees L. M. C. and Commandeur, Susanna and Dolganov, Gregory and Kramnik, Igor and Schoolnik, Gary K. and Comas, Inaki and Lund, Ole and Prins, Corine and van den Eeden, Susan J. F. and Korsvold, Gro E. and Oftung, Fredrik and Geluk, Annemieke and Ottenhoff, Tom H. M.},
	year = {2016},
	pmid = {27892960},
	pmcid = {PMC5125271},
	keywords = {Mycobacterium tuberculosis, Gene Expression Regulation, Humans, Animals, Algorithms, Up-Regulation, Mice, Inbred C57BL, Genome, Human, Computer Simulation, Antigens, Bacterial, Cytokines, Cell Proliferation, Leukocytes, Mononuclear, Cohort Studies, Immunodominant Epitopes, T-Lymphocytes},
	pages = {37793}

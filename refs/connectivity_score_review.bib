	journal = {Medycyna Doswiadczalna I Mikrobiologia},
	author = {Zdzalik, Daria and Dominiak, Aleksandra and Gałkowska, Hanna and Interewicz, Bozenna and Olszewski, Waldemar Lech and Stelmach, Ewa and Luczak, Mirosław and Machowski, Zdzisław},
	year = {2006},

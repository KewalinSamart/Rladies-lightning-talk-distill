	keywords = {circRNAs, Circular RNA, CIRI, FindCirc, Segemehl},
	pages = {55--76},
	file = {Springer Full Text PDF:/Users/jananiravi/Zotero/storage/2A8RJC5F/Sharma et al. - 2019 - Methods for Annotation and Validation of Circular .pdf:application/pdf}

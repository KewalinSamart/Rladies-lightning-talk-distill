	pmid = {30364992},
	pmcid = {PMC6323913},
	keywords = {Proteins, Genome, Databases, Protein, Metabolic Networks and Pathways, Proteome, Genome, Microbial, Multiprotein Complexes},
	pages = {D564--D572}
}

@article{allicock_baccapseq_2018,

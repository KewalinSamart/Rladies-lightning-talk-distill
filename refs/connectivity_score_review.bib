	keywords = {Mycobacterium tuberculosis, Humans, Animals, Mice, Sequence Alignment, Genome, Bacterial, Binding Sites, Sequence Homology, Nucleic Acid, Computer Graphics, DNA, Bacterial, Conserved Sequence, Transcription Factors, Mycobacterium leprae},
	pages = {472--477}
}


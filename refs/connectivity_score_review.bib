	year = {2018},
	pmid = {29277862},
	keywords = {Computational Biology, Software, Databases, Genetic, Genomics, Cluster Analysis, Algorithms, Sequence Analysis, DNA, Phylogeny, Accessory genome, Core genome, Genome, Microbial, Comparative genomics, Pan-genomics},
	pages = {29--53}
}


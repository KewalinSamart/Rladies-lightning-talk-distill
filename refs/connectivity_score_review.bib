	author = {Vallenet, David and Belda, Eugeni and Calteau, Alexandra and Cruveiller, Stéphane and Engelen, Stefan and Lajus, Aurélie and Le Fèvre, François and Longin, Cyrille and Mornico, Damien and Roche, David and Rouy, Zoé and Salvignol, Gregory and Scarpelli, Claude and Thil Smith, Adam Alexander and Weiman, Marion and Médigue, Claudine},

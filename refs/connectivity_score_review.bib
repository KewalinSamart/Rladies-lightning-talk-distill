	keywords = {Genomics, Humans, Animals, Base Sequence, Sequence Alignment, Species Specificity, Sequence Analysis, DNA, Molecular Sequence Data, Biological Evolution, Evolution, Molecular, Genome, Phylogeny, Gene Transfer, Horizontal, Conserved Sequence, Genetic Speciation, Histidine Kinase, Protein Kinases, Genome Size, Dictyostelium, Microsatellite Repeats, Polyketide Synthases},

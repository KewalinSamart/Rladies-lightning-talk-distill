	author = {Stathias, Vasileios and Turner, John and Koleti, Amar and Vidovic, Dusica and Cooper, Daniel and Fazel-Najafabadi, Mehdi and Pilarczyk, Marcin and Terryn, Raymond and Chung, Caty and Umeano, Afoma and Clarke, Daniel J. B. and Lachmann, Alexander and Evangelista, John Erol and Ma'ayan, Avi and Medvedovic, Mario and Schürer, Stephan C.},

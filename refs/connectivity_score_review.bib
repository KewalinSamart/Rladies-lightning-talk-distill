	keywords = {Humans, Molecular Sequence Annotation, Genome, Bacterial, Chromosome Mapping, Sequence Analysis, DNA, Phylogeny, Genetic Variation, Gene Transfer, Horizontal, Mycobacterium Infections, Nontuberculous, Nontuberculous Mycobacteria, Open Reading Frames},

	author = {Bundalovic-Torma, Cedoljub and Parkinson, John},
	year = {2015},
	pmid = {26621462},
	keywords = {Genomics, Bacteria, Biological Evolution, Comparative genomics, High-Throughput Screening Assays, Genomic-context, High-throughput interaction screening, Modularity, Network biology, Prokaryotic evolution},

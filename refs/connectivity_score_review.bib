	pmcid = {PMC3531135},
	keywords = {Software, Databases, Genetic, Gene Expression Profiling, Genomics, Genome, Bacterial, Internet, Systems Integration, Bacteria, Evolution, Molecular, Enzymes, Metabolic Networks and Pathways, Synteny, Genome, Archaeal},

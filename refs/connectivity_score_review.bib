	pages = {427--445},
	file = {Springer Full Text PDF:/Users/jananiravi/Zotero/storage/KGXLLB2R/Rabajante and del Rosario - 2019 - Modeling Long ncRNA-Mediated Regulation in the Mam.pdf:application/pdf}

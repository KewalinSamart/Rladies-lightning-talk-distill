	keywords = {Molecular Sequence Annotation, Algorithms, Phylogeny, Bacterial Proteins, Databases, Protein, Proteome, Eukaryota, Sequence Analysis, Protein, Archaeal Proteins, Viral Proteins},
	pages = {D286--293}
}

@article{kearse_geneious_2012,

@incollection{martirosyan_kinetic_2019,
	address = {New York, NY},
	series = {Methods in {Molecular} {Biology}},
	title = {Kinetic {Modelling} of {Competition} and {Depletion} of {Shared} {miRNAs} by {Competing} {Endogenous} {RNAs}},
	isbn = {978-1-4939-8982-9},
	url = {https://doi.org/10.1007/978-1-4939-8982-9_15},
	abstract = {Non-coding RNAs play a key role in the post-transcriptional regulation of mRNA translation and turnover in eukaryotes. miRNAs, in particular, interact with their target RNAs through protein-mediated, sequence-specific binding, giving rise to extended and highly heterogeneous miRNA–RNA interaction networks. Within such networks, competition to bind miRNAs can generate an effective positive coupling between their targets. Competing endogenous RNAs (ceRNAs) can in turn regulate each other through miRNA-mediated crosstalk. Albeit potentially weak, ceRNA interactions can occur both dynamically, affecting, e.g., the regulatory clock, and at stationarity, in which case ceRNA networks as a whole can be implicated in the composition of the cell’s proteome. Many features of ceRNA interactions, including the conditions under which they become significant, can be unraveled by mathematical and in silico models. We review the understanding of the ceRNA effect obtained within such frameworks, focusing on the methods employed to quantify it, its role in the processing of gene expression noise, and how network topology can determine its reach.},
	language = {en},
	urldate = {2019-08-13},
	booktitle = {Computational {Biology} of {Non}-{Coding} {RNA}: {Methods} and {Protocols}},
	publisher = {Springer New York},
	author = {Martirosyan, Araks and Del Giudice, Marco and Bena, Chiara Enrico and Pagnani, Andrea and Bosia, Carla and De Martino, Andrea},
	editor = {Lai, Xin and Gupta, Shailendra K. and Vera, Julio},
	year = {2019},
	doi = {10.1007/978-1-4939-8982-9_15},
	keywords = {miRNA, ceRNA, Competition, Mathematical modeling, Sponging},
	pages = {367--409},
	file = {Springer Full Text PDF:/Users/jananiravi/Zotero/storage/Z4EVSWYY/Martirosyan et al. - 2019 - Kinetic Modelling of Competition and Depletion of .pdf:application/pdf}
}


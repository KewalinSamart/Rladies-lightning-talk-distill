	keywords = {Gene Expression Profiling, Genomics, Humans, Genome, Bacterial, Systems Integration, Computer Graphics, Bacteria, Databases, Factual, Bacterial Infections, Virulence Factors, Bacterial Proteins, Protein Interaction Mapping, Virulence},

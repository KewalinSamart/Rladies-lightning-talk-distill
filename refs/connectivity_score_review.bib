	year = {2011},
	pmid = {21565935},
	keywords = {Software, Databases, Genetic, Humans, Animals, Species Specificity, Disease, Genome, Phylogeny, Disease Models, Animal, Genetic Predisposition to Disease},
	pages = {463--473}
}


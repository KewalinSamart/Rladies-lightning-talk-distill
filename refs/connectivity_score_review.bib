	pmid = {25256964},
	keywords = {Animals, Mycobacterium bovis, Cattle, Tuberculosis, Bovine, Mycobacterium, Diagnosis, Differential, Mycobacterium Infections, Nontuberculous, Prevalence, Ruminants, Mycobacterium avium subsp. paratuberculosis, Nontuberculous mycobacteria, Paratuberculosis, Bovine TB diagnosis interference, Diagnostic Errors, Map, Mycobacterial ecology, TB-like lesions, Veterinary Medicine},

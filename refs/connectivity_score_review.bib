	year = {2007},
	pmid = {17135202},
	pmcid = {PMC1751546},
	keywords = {Animals, Internet, Proteins, Phylogeny, Amino Acid Sequence, User-Computer Interface, Databases, Protein, Sequence Analysis, Protein, Protein Structure, Tertiary, Conserved Sequence},

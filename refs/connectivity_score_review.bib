	keywords = {lncRNA, ceRNA, Chromatin interactions, Disease-gene prediction, Function prediction, Network analysis},
	pages = {301--321},
	file = {Springer Full Text PDF:/Users/jananiravi/Zotero/storage/44U6RLVC/Piro and Marsico - 2019 - Network-Based Methods and Other Approaches for Pre.pdf:application/pdf}

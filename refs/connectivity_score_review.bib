	keywords = {Mycobacterium tuberculosis, microRNA, Humans, tuberculosis, Machine Learning, Biomarkers, Female, Male, Tuberculosis, Pulmonary, Adolescent, Adult, Young Adult, machine learning, Disease Progression, Risk, South Africa, biomarker, Circulating MicroRNA, Contact Tracing, correlate of risk, household contact, Uganda},
	pages = {661}

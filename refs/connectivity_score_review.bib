	keywords = {Animals, Models, Biological, Cyclin-Dependent Kinases, Protein-Serine-Threonine Kinases, Anaphase-Promoting Complex-Cyclosome, Mitosis, Cell-Free System, Oocytes, Xenopus laevis, Xenopus Proteins, Biological Clocks, bistability, Embryo, Nonmammalian, hysteresis, mitotic control, Protein Phosphatase 2},
	pages = {20539--20544}
}


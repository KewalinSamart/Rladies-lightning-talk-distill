	keywords = {Mycobacterium tuberculosis, Databases, Genetic, Genomics, Humans, Genome, Bacterial, Tuberculosis, Metabolic Networks and Pathways, Genetic Variation, Gene Expression Regulation, Bacterial, Online Systems, Genomic Library},
	pages = {225--235}
}

@article{shao_mgenomesubtractor_2010,

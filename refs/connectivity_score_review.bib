	keywords = {miRNA, Sequence Analysis, RNA, MicroRNAs, Databases, Nucleic Acid, Systems Integration},
	pages = {D152--157}
}

@article{kapopoulou_mycobrowser_2011,
	title = {The {MycoBrowser} portal: a comprehensive and manually annotated resource for mycobacterial genomes},
	volume = {91},
	issn = {1873-281X},
	shorttitle = {The {MycoBrowser} portal},
	doi = {10.1016/j.tube.2010.09.006},
	abstract = {In this paper, we present the MycoBrowser portal (http://mycobrowser.epfl.ch/), a resource that provides both in silico generated and manually reviewed information within databases dedicated to the complete genomes of Mycobacterium tuberculosis, Mycobacterium leprae, Mycobacterium marinum and Mycobacterium smegmatis. A central component of MycoBrowser is TubercuList (http://tuberculist.epfl.ch), which has recently benefited from a new data management system and web interface. These improvements were extended to all MycoBrowser databases. We provide an overview of the functionalities available and the different ways of interrogating the data then discuss how both the new information and the latest features are helping the mycobacterial research communities.},

	editor = {Lai, Xin and Gupta, Shailendra K. and Vera, Julio},
	year = {2019},
	doi = {10.1007/978-1-4939-8982-9_6},
	keywords = {miRNA, Bioinformatics, lncRNA, 3′UTR, A-to-I RNA editing, Introns, ncRNA editing, NGS},
	pages = {133--174},
	file = {Springer Full Text PDF:/Users/jananiravi/Zotero/storage/AXC4DGCP/Nigita et al. - 2019 - ncRNA Editing Functional Characterization and Com.pdf:application/pdf}
}


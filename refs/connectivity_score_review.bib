	keywords = {Computational Biology, Databases, Genetic, Animals, Mycobacterium bovis, Cattle, Internet, Tuberculosis, Bovine, DNA, Bacterial, Molecular Typing, Minisatellite Repeats, Search Engine, Bacterial Typing Techniques, Phylogeography, National Health Programs, Spain},

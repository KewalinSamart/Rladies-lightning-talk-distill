	pmid = {31053848},
	pmcid = {PMC6602458},
	keywords = {Computational Biology, Software, Databases, Genetic, Genomics, Humans, Animals, Molecular Sequence Annotation, Plants, Internet, Bacteria, Genome, Fungi},
	pages = {W52--W58}
}

@article{lemoine_ngphylogenyfr_2019,
	title = {{NGPhylogeny}.fr: new generation phylogenetic services for non-specialists},
	volume = {47},

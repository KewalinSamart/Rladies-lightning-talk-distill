	pmcid = {PMC3853122},
	keywords = {Humans, Base Sequence, Sequence Analysis, DNA, Bioinformatics, Genotype, DNA, Bacterial, Phylogeny, Polymorphism, Single Nucleotide, Bacterial Proteins, Genetic Variation, Mycobacterium Infections, Nontuberculous, Nontuberculous Mycobacteria, Bacterial Typing Techniques, NTM, cystic fibrosis, Comparative genomics, Mycobacterium abscessus, Bacteriophages, single nucleotide polymorphism, SNP, Brazil, CF, Epidemics, indel, insertion/deletion, Lung, MAB, MAB-A, MAB-B, Mycobacterium abscessus subsp. abscessus, Mycobacterium abscessus subsp. bolletii, Nontuberculous mycobacteria, PFGE, pulse-field gel electrophoresis, Skin Diseases, Bacterial},
	pages = {292--297}

	keywords = {Saccharomyces cerevisiae, Protein Binding, Adenosine Triphosphate, Adenosine Triphosphatases, Saccharomyces cerevisiae Proteins, Endosomal Sorting Complexes Required for Transport, Endosomes, Intracellular Membranes, Vesicular Transport Proteins, Transport Vesicles, Cell Division},
	pages = {172--177}
}

@article{eberhardt_shoct_2013-1,
